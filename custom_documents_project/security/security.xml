<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="project_warehouse_rule" model="ir.rule">
            <field name="name">Project Documents Warehouse Rule</field>
            <field name="model_id" ref="model_documents_document"/>
            <!-- <field name="groups" eval="[(4, ref('group_admin_warehouse'))]"/> -->
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_unlink" eval="True"/>
            <field name="domain_force">['|', ('warehouse_id', '=', False),'|', ('warehouse_id', 'in', [warehouse.id for warehouse in user.property_warehouse_ids] ), ('create_uid', '=', user.id)]</field>
            <!-- <field name="domain_force">[('warehouse_id', '=', user.property_warehouse_id.id)]</field> -->
        </record>
    </data>
</odoo>