        </record>

        <!-- <record id="group_admin_warehouse" model="res.groups">
            <field name="name">Administrador Warehouse</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record> -->

        <record id="project_warehouse_rule" model="ir.rule">
            <field name="name">Project Warehouse Rule</field>
            <field name="model_id" ref="model_project_project"/>
            <!-- <field name="groups" eval="[(4, ref('group_admin_warehouse'))]"/> -->
            <field name="perm_read" eval="True"/>
            <field name="perm_write" eval="True"/>
            <field name="perm_create" eval="True"/>
            <field name="perm_unlink" eval="True"/>
            <field name="domain_force">['|', ('warehouse_id', '=', user.property_warehouse_id.id), ('create_uid', '=', user.id)]</field>
            <!-- <field name="domain_force">[('warehouse_id', '=', user.property_warehouse_id.id)]</field> -->
        </record>
    </data>
